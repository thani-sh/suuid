{
  "name": "@thani-sh/suuid",
  "version": "1.0.0",
  "description": "Short UUIDs - UUID wrapper with base62 encoding for shorter string representation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "node --import tsx --test src/index.spec.ts"
  },
  "keywords": [
    "uuid",
    "suuid",
    "short-uuid",
    "identifier",
    "base62"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
